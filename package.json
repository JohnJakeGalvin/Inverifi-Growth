    "typedoc-plugin-merge-modules": "^4.0.1",
    "typedoc-plugin-rename-defaults": "^0.6.4",
    "typescript": "^4.8.3"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "craco build",
    "lint:editorconfig": "editorconfig-checker",
    "lint:fs": "npx ls-lint",
    "lint:css": "npx stylelint \"src/**/*.scss\"",
    "lint:js": "eslint --ext .js,jsx,.ts,.tsx src",
    "lint": "yarn lint:fs && yarn lint:css && yarn lint:js",
    "dev-config": "mv public/tenant/app-config.json public/tenant/app-config.live.json; mv public/tenant/app-config.development.json public/tenant/app-config.json",
    "staging-config": "mv public/tenant/app-config.json public/tenant/app-config.live.json; mv public/tenant/app-config.staging.json public/tenant/app-config.json",
    "codegen": "graphql-codegen --config codegen.yml",
    "docs": "grunt typedoc",
    "cypress:open": "npx cypress open",
    "cypress:run": "npx cypress run --browser chrome",
    "cypress:coverage": "npx nyc report --reporter=lcov",
    "toc": "markdown-toc -i readme.md"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "simple-git-hooks": {
    "pre-commit": "npx lint-staged"
  },
  "msw": {
    "workerDirectory": "public"
  },
  "devDependencies": {
    "@graphql-codegen/typescript-operations": "2.5.3",
    "@graphql-codegen/typescript": "2.7.3",
    "@graphql-codegen/typescript-react-apollo": "3.3.3",
    "@graphql-codegen/introspection": "2.2.1",
    "@graphql-codegen/cli": "2.12.0"
  }
}
